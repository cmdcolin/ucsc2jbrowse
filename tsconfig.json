{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "outDir": "dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "nodenext",
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "moduleResolution": "nodenext"
  }
}
